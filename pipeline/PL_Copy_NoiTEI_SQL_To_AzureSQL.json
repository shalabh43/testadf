{
	"name": "PL_Copy_NoiTEI_SQL_To_AzureSQL",
	"properties": {
		"description": "Copy calibration data from Noida TEI on premise SQL DB to Azure SQL DB",
		"activities": [
			{
				"name": "COPY_Display_Calibration_To_Azure_SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DS_SqlTB_ColorCali",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SqlTB_ColorCali",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "LVX_Calibration_PPL"
		},
		"annotations": []
	}
}